<!DOCTYPE html>
<html lang="en-US" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--
    Document Title
    =============================================
    -->
    <title>MediStock</title>
    <!--
    Favicons
    =============================================
    -->
    <link rel="apple-touch-icon" sizes="57x57" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="~/Content/Themes/Store/assets/images/favicons/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="~/Content/Themes/Store/assets/images/favicons/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/Content/Themes/Store/assets/images/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/Content/Themes/Store/assets/images/favicons/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/Themes/Store/assets/images/favicons/favicon-16x16.png">
    <link rel="manifest" href="~/Content/Themes/Store/assets/images/favicons/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="~/Content/Themes/Store/assets/images/favicons/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
    <!--
    Stylesheets
    =============================================

    -->
    <!-- Default stylesheets-->
    <link href="~/Content/Themes/Store/assets/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Template specific stylesheets-->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Volkhov:400i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/animate.css/animate.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/components-font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/et-line-font/et-line-font.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/flexslider/flexslider.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/owl.carousel/dist/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/owl.carousel/dist/assets/owl.theme.default.min.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/magnific-popup/dist/magnific-popup.css" rel="stylesheet">
    <link href="~/Content/Themes/Store/assets/lib/simple-text-rotator/simpletextrotator.css" rel="stylesheet">
    <!-- Main stylesheet and color file-->
    <link href="~/Content/Themes/Store/assets/css/style.css" rel="stylesheet">
    <link id="color-scheme" href="~/Content/Themes/Store/assets/css/colors/default.css" rel="stylesheet">
</head>
<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">

    <main>
        <div class="page-loader">
            <div class="loader">Hang on...</div>
        </div>
        <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#custom-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="@Url.Action("Index","Home")">MediStock</a>
                </div>
                <div class="collapse navbar-collapse" id="custom-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a class="section-scroll" href="@Url.Action("Index","Home")">Home</a></li>
                        <li><a class="section-scroll" href="@Url.Action("Medicine","Product")">Products</a></li>
                        <li><a class="section-scroll" href="@Url.Action("AboutUs","Home")">About Us</a></li>
                        <li><a class="section-scroll" href="@Url.Action("Auth","Authentication")">@TempData["SuccessMessage"]</a></li>
                        @if(TempData["SuccessMessage"] == null)
                        {
                        <li><a class="section-scroll" href="@Url.Action("Auth","Authentication")">Sign In</a></li>
                        }
                        <li><a class="section-scroll" href="@Url.Action("ShoppingCart","Cart")">Cart</a></li>                        
                    </ul>
                </div>
            </div>
        </nav>
        @RenderBody()
        <hr class="divider-d">
        <footer class="footer bg-dark">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="footer-social-links">
                            <a href="https://www.facebook.com/Mr.RajPawar/" target="_blank"><i class="fa fa-facebook"></i></a>
                            <a href="https://twitter.com/RajPawarr" target="_blank"><i class="fa fa-twitter"></i></a>
                            <a href="https://github.com/r-a-j" target="_blank"><i class="fa fa-github"></i></a>
                            <a href="https://www.linkedin.com/in/rajpawar/" target="_blank"><i class="fa fa-linkedin"></i></a>
                            <a href="https://www.instagram.com/raj.pwr/" target="_blank"><i class="fa fa-instagram"></i></a>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <p class="copyright font-alt">&copy; 2020&nbsp;<a href="@Url.Action("Index","Home")">MediStock</a>, All Rights Reserved</p>
                    </div>
                </div>
            </div>
        </footer>
        <div class="scroll-up"><a href="#totop"><i class="fa fa-angle-double-up"></i></a></div>
    </main>
    <!--
    JavaScripts
    =============================================
    -->
    <script src="~/Content/Themes/Store/assets/lib/jquery/dist/jquery.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/wow/dist/wow.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/jquery.mb.ytplayer/dist/jquery.mb.YTPlayer.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/isotope/dist/isotope.pkgd.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/imagesloaded/imagesloaded.pkgd.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/flexslider/jquery.flexslider.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/owl.carousel/dist/owl.carousel.min.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/smoothscroll.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/magnific-popup/dist/jquery.magnific-popup.js"></script>
    <script src="~/Content/Themes/Store/assets/lib/simple-text-rotator/jquery.simple-text-rotator.min.js"></script>
    <script src="~/Content/Themes/Store/assets/js/plugins.js"></script>
    <script src="~/Content/Themes/Store/assets/js/main.js"></script>
</body>
</html>